{
  "name": "apex-arbitrage-system",
  "version": "2.0.0",
  "description": "Production-ready multi-DEX flash loan arbitrage system with Rust engine, AI/ML models, and multi-chain support",
  "main": "src/apex-production-runner.js",
  "type": "module",
  "scripts": {
    "setup": "yarn install --network-timeout 600000 && echo âœ… Dependencies installed successfully.",
    "build": "yarn run format && yarn run lint",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --fix",
    "start": "yarn start:all",
    "start:all": "concurrently \"yarn start:node\" \"yarn start:python\"",
    "start:node": "node src/dex_pool_fetcher.js",
    "start:python": "python src/main_deploy_launcher.py",
    "start:telemetry": "python src/cross_chain_telemetry.py",
    "start:executor": "python src/executor_raptor_4x4x4x4.py",
    "dryrun": "python scripts/test_dryrun.py",
    "health": "python src/health_monitor.py",
    "verify": "python scripts/system_integrity_audit.py",
    "validate": "node scripts/comprehensive-validation.js",
    "validate:full": "node scripts/comprehensive-validation.js",
    "logs": "tail -f logs/system.log",
    "clean": "rimraf node_modules && echo ðŸ§¹ Cleaned node_modules directory.",
    "rebuild": "yarn clean && yarn install",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "deploy": "python src/main_deploy_launcher.py --mode=production",
    "simulate": "python src/main_deploy_launcher.py --mode=simulation",
    "dev": "node --watch src/index.js",
    "build:rust": "cd src/rust && cargo build --release",
    "test": "node --test tests/*.test.js",
    "deploy": "hardhat run scripts/deploy.js",
    "verify": "node scripts/verify-setup.js",
    "verify:wiring": "node scripts/validate-complete-wiring.js",
    "verify:websocket": "node scripts/validate-websocket-connections.js",
    "verify:all": "node scripts/validate-complete-wiring.js && node scripts/validate-websocket-connections.js",
    "test:integration": "node tests/integration-end-to-end.test.js",
    "ai:start": "python3 src/python/omni_mev_ai_engine.py",
    "ai:dev": "uvicorn src.python.omni_mev_ai_engine:app --reload --port 8001"
  },
  "keywords": [
    "arbitrage",
    "defi",
    "flash-loan",
    "polygon",
    "ethereum",
    "flashloan",
    "rust",
    "machine-learning"
  ],
  "author": "Apex Trading System",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "moment": "^2.30.1",
    "prettier": "^3.3.3",
    "web3": "^4.16.0",
    "better-sqlite3": "^9.2.2",
    "node-telegram-bot-api": "^0.64.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^9.8.0",
    "rimraf": "^6.0.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "mocha": "^10.2.0",
    "chai": "^4.3.10"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  }
}
