{
  "name": "apex-arbitrage-system",
  "version": "2.0.0",
  "description": "Production-ready multi-DEX flash loan arbitrage system with Rust engine, AI/ML models, and multi-chain support",
  "main": "src/apex-production-runner.js",
  "type": "module",
  "scripts": {
    "setup": "yarn install --network-timeout 600000 && echo âœ… Dependencies installed successfully.",
    "build": "yarn run format && yarn run lint",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --fix",
    "start": "yarn start:all",
    "start:all": "concurrently \"yarn start:node\" \"yarn start:python\"",
    "start:node": "node src/dex_pool_fetcher.js",
    "start:python": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python src/main_deploy_launcher.py'",
    "start:telemetry": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python src/cross_chain_telemetry.py'",
    "start:executor": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python src/executor_raptor_4x4x4x4.py'",
    "dryrun": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python scripts/test_dryrun.py'",
    "health": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python src/health_monitor.py'",
    "verify": "node scripts/verify-installer.js",
    "verify:installer": "bash tests/test-one-click-installer.sh",
    "validate": "node scripts/comprehensive-validation.js",
    "validate:full": "node scripts/comprehensive-validation.js",
    "precheck": "node scripts/pre-operation-checklist.js",
    "preflight": "node scripts/pre-operation-checklist.js",
    "logs": "tail -f logs/system.log",
    "clean": "rimraf node_modules && echo ðŸ§¹ Cleaned node_modules directory.",
    "rebuild": "yarn clean && yarn install",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "deploy": "hardhat run scripts/deploy.js",
    "simulate": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python src/main_deploy_launcher.py --mode=simulation'",
    "dev": "node --watch src/index.js",
    "build:rust": "cd src/rust && cargo build --release",
    "test": "node --test tests/*.test.js",
    "test:regression": "node scripts/run-regression-tests.js",
    "validate:performance": "node scripts/validate-performance.js",
    "ai:start": "bash -c 'source .venv/bin/activate 2>/dev/null || true && python3 src/python/omni_mev_ai_engine.py'",
    "ai:dev": "bash -c 'source .venv/bin/activate 2>/dev/null || true && uvicorn src.python.omni_mev_ai_engine:app --reload --port 8001'",
    "audit:deployment": "node scripts/final-deployment-audit.js",
    "benchmark:analysis": "node scripts/benchmark-analysis.js",
    "audit:full": "node scripts/final-deployment-audit.js && node scripts/benchmark-analysis.js"
  },
  "keywords": [
    "arbitrage",
    "defi",
    "flash-loan",
    "polygon",
    "ethereum",
    "flashloan",
    "rust",
    "machine-learning"
  ],
  "author": "Apex Trading System",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "moment": "^2.30.1",
    "node-telegram-bot-api": "^0.64.0",
    "prettier": "^3.3.3",
    "web3": "^4.16.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "hardhat": "^3.0.9",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  }
}
